        "@radix-ui/react-dialog": "^1.1.6",
        "@radix-ui/react-label": "^2.1.2",
        "@radix-ui/react-popover": "^1.1.6",
        "@radix-ui/react-select": "^2.1.6",
        "@radix-ui/react-separator": "^1.1.2",
        "@radix-ui/react-slot": "^1.1.2",
        "@radix-ui/react-switch": "^1.1.3",
        "axios": "^1.7.7",
        "class-variance-authority": "^0.7.1",
        "clsx": "*",
        "cmdk": "^1.1.1",
        "embla-carousel-react": "^8.6.0",
        "input-otp": "^1.4.2",
        "lucide-react": "^0.487.0",
        "next-themes": "^0.4.6",
        "react": "^18.3.1",
        "react-day-picker": "^8.10.1",
        "react-dom": "^18.3.1",
        "react-hook-form": "^7.55.0",
        "react-resizable-panels": "^2.1.7",
        "recharts": "^2.15.2",
        "sonner": "^2.0.3",
        "tailwind-merge": "*",
        "vaul": "^1.1.2"
    },
    "devDependencies": {
        "@jest/globals": "^30.2.0",
        "@pact-foundation/pact": "^16.0.4",
        "@playwright/test": "^1.57.0",
        "@testing-library/dom": "^10.4.1",
        "@testing-library/jest-dom": "^6.9.1",
        "@testing-library/react": "^16.3.1",
        "@testing-library/user-event": "^14.6.1",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jest": "^30.0.0",
        "@types/node": "^20.10.0",
        "@types/react": "^19.2.7",
        "@types/react-dom": "^19.2.3",
        "@vitejs/plugin-react-swc": "^3.10.2",
        "@vitest/ui": "1.6.1",
        "cors": "^2.8.5",
        "cypress": "^13.13.1",
        "cypress-multi-reporters": "^2.0.5",
        "dotenv": "^17.2.3",
        "express": "^4.18.2",
        "happy-dom": "^20.1.0",
        "jest": "^30.2.0",
        "junit-report-merger": "^9.0.3",
        "mocha": "10",
        "mocha-junit-reporter": "^2.2.1",
        "mocha-multi-reporters": "^1.5.1",
        "mochawesome": "^7.1.4",
        "mochawesome-merge": "^5.1.0",
        "mochawesome-report-generator": "^6.3.2",
        "ts-jest": "^29.4.6",
        "ts-node": "^10.9.2",
        "tsx": "^4.21.0",
        "typescript": "^5.6.3",
        "vite": "6.3.5",
        "vite-plugin-pwa": "^1.2.0",
        "vitest": "^1.6.0"
    },
    "scripts": {
        "dev": "vite",
        "dev:mock": "VITE_USE_MOCK_API=true vite",
        "build": "vite build",
        "test": "vitest",
        "test:ui": "vitest --ui",
        "test:run": "vitest run",
        "test:coverage": "vitest run --coverage",
        "test:e2e:cypress:run": "cypress run --config-file tests/cypress.config.ts",
        "test:e2e:cypress:open": "cypress open --config-file tests/cypress.config.ts",
        "test:e2e:cypress:mochawesome:merge": "mochawesome-merge tests/cypress/reports/mochawesome/*.json > tests/cypress/reports/mochawesome/merged.json",
        "test:e2e:cypress:mochawesome:html": "marge tests/cypress/reports/mochawesome/merged.json -f final-report -o tests/cypress/reports/mochawesome/html",
        "test:e2e:cypress:junit:merge": "tsx tests/cypress/helpers/merger.ts",
        "test:e2e:cypress:merge:all": "yarn cypress:report:merge:mochawesome && yarn cypress'report:html:mochawesome && yarn cypress:report:merge:junit",
        "test:e2e:playwright:run": "playwright test --config=tests/playwright.config.ts --grep-invert /contract/",
        "test:e2e:playwright:ui": "playwright test --config=tests/playwright.config.ts --grep-invert /contract/ --ui",
        "test:e2e:playwright:debug": "playwright test --config=tests/playwright.config.ts --grep-invert /contract/ --debug",
        "test:e2e:playwright:report": "playwright show-report tests/playwright/reports/html",
        "test:contract:validation": "playwright test tests/playwright/contract --config=tests/playwright.config.ts",
        "test:contract:pact": "jest --config=jest.config.pact.js",
        "test:contract:pact:watch": "jest --config=jest.config.pact.js --watch",
        "test:contract:pact:verbose": "jest --config=jest.config.pact.js --verbose",
        "test:k6:mock-server": "tsx tests/mock-server.ts",
        "test:k6:browser": "k6 run tests/k6/performance.test.js",
        "test:k6:api": "k6 run tests/k6/api-errors.test.js",
        "test:k6:all": "npm run test:k6:browser && npm run test:k6:api"
    }
}
